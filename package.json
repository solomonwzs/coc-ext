{
  "name": "coc-solomon-ext",
  "version": "0.0.1-development",
  "description": "",
  "author": "solomon <solomon.wzs@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": ["coc.nvim"],
  "engines": {
    "coc": "^0.0.80"
  },
  "scripts": {
    "lint": "eslint src --ext ts",
    "clean": "rimraf lib",
    "watch": "node esbuild.js --watch",
    "build": "node esbuild.js",
    "prepare": "npm-run-all clean build"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "semi": true
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "coc.nvim": "^0.0.80",
    "esbuild": "^0.8.46",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2"
  },
  "activationEvents": ["*"],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "coc-solomon-ext configuration",
      "properties": {
        "coc-solomon-ext.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable coc-solomon-ext extension"
        },
        "coc-solomon-ext.test": {
          "type": "string",
          "default": "test",
          "description": "test"
        }
      }
    },
    "commands": [
      {
        "command": "coc-solomon-ext.Command",
        "title": "coc-solomon-ext command title"
      }
    ]
  },
  "dependencies": {
    "@types/node": "^14.14.30",
    "request-light": "^0.4.0"
  }
}
